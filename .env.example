# KrishiRakshak — Environment Variables
# Copy this file to .env and update values as needed

# API Gateway URL (from Lambda deployment)
# Leave empty or comment out to use demo mode
VITE_API_GATEWAY_URL=https://your-api-id.execute-api.ap-south-1.amazonaws.com/prod

# AWS Region
VITE_AWS_REGION=ap-south-1

# App version
VITE_APP_VERSION=1.0.0

# Demo mode — set to "true" to use built-in mock data (no AWS needed)
# Automatically enabled if VITE_API_GATEWAY_URL is empty or contains "your-api-id"
VITE_DEMO_MODE=true
